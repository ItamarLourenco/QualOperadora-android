<resources>

    <style name="Theme.Qualoperadora" parent="@style/Theme.AppCompat.Light">
        <item name="android:windowContentOverlay">@null</item>
        <item name="actionBarItemBackground">@drawable/selectable_background_qualoperadora</item>
        <item name="popupMenuStyle">@style/PopupMenu.Qualoperadora</item>
        <item name="dropDownListViewStyle">@style/DropDownListView.Qualoperadora</item>
        <item name="actionBarTabStyle">@style/ActionBarTabStyle.Qualoperadora</item>
        <item name="actionDropDownStyle">@style/DropDownNav.Qualoperadora</item>
        <item name="actionBarStyle">@style/ActionBar.Solid.Qualoperadora</item>
        <item name="actionModeBackground">@drawable/cab_background_top_qualoperadora</item>
        <item name="actionModeSplitBackground">@drawable/cab_background_bottom_qualoperadora</item>
        <item name="actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Qualoperadora</item>
    </style>

    <style name="Theme.QualoperadoraDark" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="actionBarItemBackground">@drawable/selectable_background_qualoperadora</item>
        <item name="popupMenuStyle">@style/PopupMenu.Qualoperadora</item>
        <item name="dropDownListViewStyle">@style/DropDownListView.Qualoperadora</item>
        <item name="actionBarTabStyle">@style/ActionBarTabStyle.Qualoperadora</item>
        <item name="actionDropDownStyle">@style/DropDownNav.Qualoperadora</item>
        <item name="actionBarStyle">@style/ActionBar.Solid.Qualoperadora</item>
        <item name="actionModeBackground">@drawable/cab_background_top_qualoperadora</item>
        <item name="actionModeSplitBackground">@drawable/cab_background_bottom_qualoperadora</item>
        <item name="actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Qualoperadora</item>
    </style>


    <style name="ActionBar.Solid.Qualoperadora" parent="@style/Widget.AppCompat.Light.ActionBar.Solid">
        <item name="background">@drawable/ab_solid_qualoperadora</item>
        <item name="backgroundStacked">@drawable/ab_stacked_solid_qualoperadora</item>
        <item name="backgroundSplit">@drawable/ab_bottom_solid_qualoperadora</item>
        <item name="progressBarStyle">@style/ProgressBar.Qualoperadora</item>
    </style>

    <style name="ActionBar.Transparent.Qualoperadora" parent="@style/Widget.AppCompat.Light.ActionBar">
        <item name="background">@drawable/ab_transparent_qualoperadora</item>
        <item name="progressBarStyle">@style/ProgressBar.Qualoperadora</item>
    </style>

    <style name="CheckBoxQualoperadora" parent="android:Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/apptheme_btn_check_holo_light</item>
    </style>

    <style name="PopupMenu.Qualoperadora" parent="@style/Widget.AppCompat.Light.PopupMenu">
    </style>

    <style name="DropDownListView.Qualoperadora" parent="@style/Widget.AppCompat.Light.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_qualoperadora</item>
    </style>

    <style name="ActionBarTabStyle.Qualoperadora" parent="@style/Widget.AppCompat.Light.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_qualoperadora</item>
    </style>

    <style name="DropDownNav.Qualoperadora" parent="@style/Widget.AppCompat.Light.Spinner.DropDown.ActionBar">
        <item name="android:background">@drawable/spinner_background_ab_qualoperadora</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_qualoperadora</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_qualoperadora</item>
    </style>

    <style name="ProgressBar.Qualoperadora" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_qualoperadora</item>
    </style>

    <style name="ActionButton.CloseMode.Qualoperadora" parent="@style/Widget.AppCompat.Light.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_qualoperadora</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Qualoperadora.Widget" parent="@style/Theme.AppCompat">
        <item name="popupMenuStyle">@style/PopupMenu.Qualoperadora</item>
        <item name="dropDownListViewStyle">@style/DropDownListView.Qualoperadora</item>
    </style>

</resources>
